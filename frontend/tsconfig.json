{
  "extends": "./tsconfig.paths.json",
  "compilerOptions":{
     "target":"es5",
     "module":"es6",
     "moduleResolution":"node",
     "lib":[
        "es6",
        "dom"
     ],
     "sourceMap":true,
     "jsx":"react-jsx",
     "strict":true,
     "noImplicitReturns":true,
     "baseUrl": "src",
  },
  "include":[
     "**/*.ts",
     "**/*.tsx"
  ],
  "exclude":[
     "node_modules"
  ]
}